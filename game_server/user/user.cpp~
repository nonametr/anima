#include "user.h"

User::User()
{

}
User::~User()
{

}
///--------------SETTERS----------------
void User::set(string key, Value &value)
{
    _values[key] = value;
}
///-------------GETTERS-----------------
Value User::get(string key)
{
    auto it = _values.find(key);
    if (it != _values.end())
    {
        return it->second;
    }
    return Value(Value::UNKNOWN);
};
